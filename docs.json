  "interaction": {
    "drilldown": true
  },
  "api": {
    "params": {
      "expanded": "closed"
    },
    "playground": {
      "display": "interactive",
      "proxy": false
    },
    "examples": {
      "languages": [
        "curl",
        "python",
        "javascript"
      ],
      "defaults": "required",
      "prefill": true
    },
    "mdx": {
      "auth": {
        "method": "basic",
        "name": "Basic"
      }
    }
  },
  "seo": {
    "metatags": {
      "facebook": "to replace",
      "twitter": "to replace"
    },
    "indexing": "all"
  },
  "search": {
    "prompt": "Explain this for me pls"
  },
  "integrations": {
    "intercom": {
      "appId": "replace"
    }
  },
  "errors": {
    "404": {
      "redirect": true,
      "title": "someone took the wrong turn 🌝",
      "description": "![404](./images/confused-monkey.jpg)\n\nWe could make the best of the 404 page and use it to capture new leads ;)"
    }
  },
